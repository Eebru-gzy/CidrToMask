pipeline {
    agent any

    stages {
        stage('Build') {
          steps {
            sh '''
                'Hello World'
                '''
          }
        }
        stage('Test') {
          steps {
              sh 'echo Hello Test'
          }
        }
        stage('Push') {
          steps {
              sh 'echo Hello Push'
          }
        }
        stage('Deploy') {
          steps {
              sh 'echo Hello Deploy'
          }
        }
    }
}
